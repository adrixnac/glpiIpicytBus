<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity Ticket.
    -->
    <changeSet id="20201202041546-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="transaction_id"
                                 baseTableName="ticket"
                                 constraintName="fk_ticket_transaction_id"
                                 referencedColumnNames="id"
                                 referencedTableName="transaction"/>

        <addForeignKeyConstraint baseColumnNames="subtype_transaction_id"
                                 baseTableName="ticket"
                                 constraintName="fk_ticket_subtype_transaction_id"
                                 referencedColumnNames="id"
                                 referencedTableName="subtype_transaction"/>

        <addForeignKeyConstraint baseColumnNames="product_cat01_id"
                                 baseTableName="ticket"
                                 constraintName="fk_ticket_product_cat01_id"
                                 referencedColumnNames="id"
                                 referencedTableName="product_cat"/>

        <addForeignKeyConstraint baseColumnNames="product_cat02_id"
                                 baseTableName="ticket"
                                 constraintName="fk_ticket_product_cat02_id"
                                 referencedColumnNames="id"
                                 referencedTableName="product_cat"/>

        <addForeignKeyConstraint baseColumnNames="product_cat03_id"
                                 baseTableName="ticket"
                                 constraintName="fk_ticket_product_cat03_id"
                                 referencedColumnNames="id"
                                 referencedTableName="product_cat"/>

        <addForeignKeyConstraint baseColumnNames="contact_types_id"
                                 baseTableName="ticket"
                                 constraintName="fk_ticket_contact_types_id"
                                 referencedColumnNames="id"
                                 referencedTableName="contact_type"/>

        <addForeignKeyConstraint baseColumnNames="impacts_id"
                                 baseTableName="ticket"
                                 constraintName="fk_ticket_impacts_id"
                                 referencedColumnNames="id"
                                 referencedTableName="impact"/>

        <addForeignKeyConstraint baseColumnNames="urgencies_id"
                                 baseTableName="ticket"
                                 constraintName="fk_ticket_urgencies_id"
                                 referencedColumnNames="id"
                                 referencedTableName="urgency"/>

    </changeSet>
</databaseChangeLog>
